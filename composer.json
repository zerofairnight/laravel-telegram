{
    "name": "zerofairnight/laravel-telegram",
    "description": "Telegram Bot for Laravel",
    "keywords": ["telegram", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "zerofairnight",
            "email": "zerofairnight@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Laravel\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Telegram\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Telegram\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "Laravel\\Telegram\\Facades\\Telegram"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
